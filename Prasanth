#include <stdio.h>
#include <math.h>
int main() {
    double number, squareRoot;
    printf("Enter a number: ");
    scanf("%lf", &number);
     squareRoot = sqrt(number);
     printf("Square root of %.2lf = %.2lf\n", number, squareRoot);
     return 0;
}
[12/03, 1:46 pm] Prasanth: Program 8 
#include <stdio.h>
int main() {
    int num1, num2;
    
    printf("Enter first number: ");
    scanf("%d", &num1);
    
    printf("Enter second number: ");
    scanf("%d", &num2);
    
    if (num1 == num2) {
        printf("Both numbers are equal.\n");
    } else if (num1 < num2) {
        printf("Smallest number: %d\n", num1);
        printf("Biggest number: %d\n", num2);
    } else {
        printf("Smallest number: %d\n", num2);
        printf("Biggest number: %d\n", num1);
    }
    
    return 0;
}
[12/03, 1:46 pm] Prasanth: Program 9
#include <stdio.h>

int main() {
    int num1, num2, num3;
    
    printf("Enter three distinct numbers: ");
    scanf("%d %d %d", &num1, &num2, &num3);
    
    if (num1 < num2 && num1 < num3) {
        printf("Smallest number: %d\n", num1);
    } else if (num2 < num1 && num2 < num3) {
        printf("Smallest number: %d\n", num2);
    } else {
        printf("Smallest number: %d\n", num3);
    }
    
    return 0;
}
